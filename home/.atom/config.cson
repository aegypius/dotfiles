"*":
  editor:
    showInvisibles: true
    showIndentGuide: true
    scrollPastEnd: true
    invisibles: {}
  core:
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "Thumbs.db"
      "app/cache"
      "_cache"
      "_tmp"
      "_tools"
      "cgi-bin"
      ".vagrant"
      "userfiles"
      ".capistrano"
      "_builds"
      "_projects"
      "_steps"
    ]
    excludeVcsIgnoredPaths: false
    disabledPackages: [
      "linter-phpmd"
      "language-twig"
      "linter-jshint"
      "linter-jsxhint"
      "spell-check"
    ]
    themes: [
      "one-dark-ui"
      "atom-dark-syntax"
    ]
    autoHideMenuBar: true
    projectHome: "~/src/github.com/aegypius/"
  "exception-reporting":
    userId: "042b8423-000c-3604-2877-fa18bc8ce717"
  "release-notes":
    viewedVersion: "0.1.0"
  welcome:
    showOnStartup: false
  metrics:
    userId: "5afe01d925f56ca54ea8945b2cce677f8755be3a"
  "atom-beautify":
    _analyticsUserId: "a6669e24-585f-4be5-abcf-1009aef1b8a8"
    html_brace_style: "expand"
    html_indent_inner_html: true
  "tree-view":
    hideIgnoredNames: true
  "project-manager":
    sortBy: "group"
  "file-icons": {}
  linter:
    showErrorInline: true
    subtleLinterErrors: [
      "jscs"
    ]
    ignoredLinterErrors: [
      "jsonlint"
      "php"
      "phpcs"
    ]
    errorPanelHeight: 129
  docblockr:
    per_section_indent: true
  "git-diff": {}
  "spell-check": {}
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
  "language-gentoo":
    updateManifestOnSave: true
  "linter-phpcs": {}
  "mocha-test-runner":
    nodeBinaryPath: "/usr/bin/node"
  "fuzzy-finder":
    ignoredNames: [
      "app/cache*"
    ]
  "file-types":
    Cargofile$: "source.coffee"
    thtm: "source.html"
    gotpl: "source.gotemplate"
  react: {}
  emmet: {}
  "one-dark-ui":
    layoutMode: "Spacious"
  tabs:
    usePreviewTabs: true
    enableVcsColoring: true
  "linter-jscs":
    harmony: true
    onlyConfig: true
  "linter-eslint":
    disableWhenNoEslintrcFileInPath: true
    useGlobalEslint: true
  "linter-less":
    includePath: [
      "vendor/bower_components"
    ]
  "atom-autocomplete-php": {}
  tablr: {}
  "php-cs-fixer": {}
  "find-and-replace":
    openProjectFindResultsInRightPane: true
